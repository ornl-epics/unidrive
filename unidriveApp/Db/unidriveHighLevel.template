record(calc, "$(s):$(ta):$(ss):AlarmSum") {
   field(INPA, "$(s):$(ta):$(ss):UD:DriveOK CPP MS")
   field(INPB, "$(s):$(ta):$(ss):DLambda:Fault CPP MS")
   field(INPC, "$(s):$(ta):$(ss):Lambda:Fault CPP MS")
   field(INPD, "$(s):$(ta):$(ss):Speed:Fault CPP MS")
   field(INPE, "$(s):$(ta):$(ss):Tilt:Fault CPP MS")
   field(CALC, "!A||B||C||D||E")
   field(HIHI, "0.5")
   field(HHSV, "MAJOR")
}

record(mbbo, "$(s):$(ta):$(ss):SpeedMode") {
   field(ZRST, "Speed")
   field(ONST, "Lambda")
   info(autosaveFields, "VAL")
}

record(ao, "$(s):$(ta):$(ss):Lambda:Set") {
   field(EGU, "A")
   alias("$(s):$(ta):$(ss):Lambda:Reqd")
   field(FLNK, "$(s):$(ta):$(ss):Lambda:SetFan")
}

record(fanout, "$(s):$(ta):$(ss):Lambda:SetFan") {
   field(LNK1, "$(s):$(ta):$(ss):TiltCalc")
   field(LNK2, "$(s):$(ta):$(ss):SpeedCalc")
}

record(calc, "$(s):$(ta):$(ss):AtVelocity") {
   field(INPA, "$(s):$(ta):$(ss):DLambdaCalc CPP MS")
   field(INPB, "$(s):$(ta):$(ss):DLambda:Set CPP MS")
   field(INPC, "0.1")
   field(INPD, "$(s):$(ta):$(ss):WavelengthCalc CPP MS")
   field(INPE, "$(s):$(ta):$(ss):Lambda:Set CPP MS")
   field(INPF, "0.1")
   field(CALC, "(ABS(A-B)<C)&&(ABS(D-E)<F)")
}

substitute "P=$(s):$(ta):$(ss)"
substitute "C=Lambda"
substitute "EGU=A"
substitute "OUT=$(s):$(ta):$(ss):Lambda:Set"
substitute "IN=$(s):$(ta):$(ss):AtVelocity"
substitute "TO=36000"
substitute "ST=15"
include "putCallback.template"

record(ao, "$(s):$(ta):$(ss):DLambda:Set") {
   field(EGU, "A")
   field(FLNK, "$(s):$(ta):$(ss):TiltCalc")
   alias("$(s):$(ta):$(ss):DLambda:Reqd")
}

record(calc, "$(s):$(ta):$(ss):AtDLambda") {
   field(INPA, "$(s):$(ta):$(ss):DLambdaCalc CPP MS")
   field(INPB, "$(s):$(ta):$(ss):DLambda:Set CPP MS")
   field(INPC, "0.1")
   field(CALC, "ABS(A-B)<C")
}

substitute "P=$(s):$(ta):$(ss)"
